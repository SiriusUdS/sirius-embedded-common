{
    "version": 4,
    "configurations": [
        {
            /** 
             * ms-vscode.cmake-tools plugin should be installed.
             * 
             * It provides data for C/C++ plugin,
             * such as include paths, browse paths, defines, etc.
             */
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/../sirius-rocket-engine/SIRIUS_Rocket_Engine/**",
                "${workspaceFolder}/../sirius-rocket-engine/SIRIUS_Rocket_Engine/Drivers/STM32F4xx_HAL_Driver/Inc",
                "${workspaceFolder}/../sirius-rocket-engine/SIRIUS_Rocket_Engine/Core/Inc"
            ],
            "defines": [
                "DEBUG",
                "STM32F411xE",
                "USE_HAL_DRIVER"
            ],
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "${default}"
        }
    ]
}